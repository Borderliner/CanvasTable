{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "chrome": "60"
      }
    }],
    "@babel/preset-typescript",
    ["@babel/preset-react"]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-optional-chaining"
  ]
}
